#content {
	position: relative;
	background: #fff;
}
.header {
	height: 100px;
}
.header .fixed-part {
	padding: 30px 0 17px;
	border-bottom: 1px solid #eef2f3;
	width: 100%;
	z-index: 100;
	background: #fff;
	height: 100px;
}
.header .fixed-part .wrap-btn-menu {
	position: relative;
	z-index: 5;
}
.header .fixed-part .wrap-btn-menu .btn-menu {
	@include menu-icon(#3B4444);
}
.header .fixed-part .wrap-btn-menu.show .btn-menu {
	@include close-menu-icon();
}

.is-fixed-header .header .fixed-part {
	position: fixed;
    top: 0;
    left: 0;
}
.header .logo {
	display: inline-block;
	width: 150px;
	height: 40px;
	background: url(../svg/logo-2.svg) center top no-repeat;
	background-size: 100% auto;
}
.header .menu {
	padding-right: 40px;
}
.header .menu ul {
	display: block;
	padding: 0;
	margin: 0;
	list-style: none;
	font-size: 18px;
	text-transform: uppercase;
	letter-spacing: 1.1px;
}
.header .menu ul li {
	display: inline-block;
	margin: 0 31px 0 0;
	padding: 0;
}
.header .menu ul li:last-child {
	margin-right: 0;
}
.header .menu ul li a {
	color: #3B4444;
	text-decoration: none;
}
.header .menu ul li a:hover, .header .menu ul li.current-menu-item a {
	color: #C2D1BC;
}
footer {
	background: #fff;
	text-align: center;
}
footer a {
	text-decoration: none;
}

footer .social a {
	display: inline-block;
	color: $main-color;
	margin: 0 3px;
}

footer .social a:hover {
	color: $a-color;
}
footer .social a:hover .fa-facebook {
	color: #365899;
}
footer .social a:hover .fa-vk {
	color: #507299;
}
footer .social a:hover .fa-twitter {
	color: #1DA1F2;
}
footer .social a:hover .fa-instagram {
	color: #262626;
}
footer .social a:hover .fa-youtube-play {
	color: #cc181e;
}


.spinner {
	position: relative;
}
.spinner input {
  text-align: right;
  padding-right: 30px;
  text-align: center;
}

.spinner .input-group-btn-vertical {
	position: absolute;
    white-space: nowrap;
    width: 30px;
    top: 0;
    right: 0;
    z-index: 10;
	height: 100%;
	background: #EEF2F3;
}

.spinner .input-group-btn-vertical > .btn-control {
	position: absolute;
    top: 0;
    width: 100%;
    height: 50%;
    right: 0;
    line-height: 25px;
    text-align: center;
    text-decoration: none;
}

.spinner .input-group-btn-vertical > .btn-control:last-child {
	top: 50%;
}



@mixin products () {
	ul {
		display: block;
		margin: 0;
		padding: 0;
		list-style: none;
	}
	ul li {
		float: left;
		clear: none;
		margin: 0 0 28px;
		padding: 0 14px;
		width: 30%;
		
	}
	ul li .wrap-border {
		border: 1px solid transparent;
	}
	ul li .inner {
		position: relative;
		border: 1px solid #eef2f3;
		overflow: hidden;
		height: 332px;
	}
	ul li:nth-of-type(9n-7) {
		width: 40%;
	}
	ul li:nth-of-type(9n-5) {
		width: 40%;
	}
	ul li:nth-of-type(9n-2) {
		width: 40%;
	}
	ul li:nth-of-type(9n-1) {
		margin-top: -118px;	
	}
	ul li:nth-of-type(9n-1) .inner {
		height: 450px;
	}

	ul li:nth-of-type(9n-5) .inner, ul li:nth-of-type(9n-3) .inner {
		height: 450px;	
	}

	ul li .inner .bg {
		position: absolute;
		display: table;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		overflow: hidden;
		text-align: center;
	}
	ul li .inner .bg img {
		max-width: 100%;
		height: auto;
	}
	ul li .inner .bottom-part {
		position: absolute;
		bottom: 0;
		left: 0;
		width: 100%;
		padding: 20px 31px 20px;
		background: rgba(255, 255, 255, 0.75);
	}
	ul li .inner .bottom-part .title {
		
	}
	ul li .inner .bottom-part .brand {
		font-size: 16px;
		line-height: 20px;
		font-style: italic;
		color: #769769;
			margin: 0 0 5px;
	}
	ul li .inner .bottom-part .brand a {
		text-decoration: none;
		color: #769769;
	}
	ul li .inner .bottom-part .price {
		font-size: 24px;
		font-weight: 600;
		color: #7B8586;
		line-height: 30px;
	}
	ul li .inner .hover-part {
		@include cros-opacity(0);
		position: absolute;
		display: table;
		width: 100%;
		height: 100%;
		background: rgba(255, 255, 255, 0.80);
		@include animation-transition (0.5s);
		text-align: center;
	}
	ul li .inner .hover-part .mobile-cover {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
	}
	ul li .inner .hover-part .btn {
		display: none;
		margin: 15px 0;
		padding-left: 0;
		padding-right: 0;
		width: 210px;
	}
	ul li .inner .hover-part .btn .added  {
		display: none;
	}
	ul li:hover .wrap-border {
		border: 1px solid #88C9CD;
	}
	ul li:hover .inner {
		border: 1px solid #88C9CD;
	}
	ul li:hover .inner .hover-part {
		@include cros-opacity(1);
	}
	ul li:hover .inner .hover-part .btn {
		display: inline-block;
	}
	@media (max-width: $media-md-max-width) {		
		ul li {
			padding: 0 10px;
		}
		ul li .inner .bottom-part {
			padding: 20px 20px 20px;
		}
		ul li .inner .bottom-part .brand {
			font-size: 12px;
		}
		ul li .inner .bottom-part .price {
			font-size: 20px;
		}
	}
	@media (max-width: $media-xs-max-width) {
		ul li {
			padding: 0 10px;
			width: 100%;
		}
		ul li:nth-of-type(9n-7) {
			width: 100%;			
		}
		ul li:nth-of-type(9n-5) {
			width: 100%;			
		}
		ul li:nth-of-type(9n-2) {
			width: 100%;			
		}
		ul li:nth-of-type(9n-5) .inner, ul li:nth-of-type(9n-3) .inner, ul li .inner {
			height: 260px;
		}
	}
}
.wrap-form-control {
	position: relative;
}
.wrap-form-control .additional-placeholder {
	position: absolute;
    top: 13px;
    left: 15px;
    font-size: 14px;
    color: #3B4444;
    font-weight: 300;
    line-height: 14px;
	z-index: 200;
}
.wrap-form-control .additional-placeholder span {
	color: #D5B9A1;
}

.wrap-form-control.wrap-datepicker .fa {
	font-size: 12px;
	position: absolute;
	top: 50%;
	right: 10px;
}



.menu-categories {
	position: fixed;
	display: none;
	top: 200px;
	left: 0;
	width: 100%;
	height: 100%;
	//background: url(../img/template-menu-categories.png) center -225px no-repeat #EEF2F3;
	background: #EEF2F3;
	z-index: 100;	
	padding-bottom: 200px;
}
.menu-categories .inner {
	position: relative;
	height: 100%;
	padding-top: 50px;
	overflow-y: scroll;
}
.menu-categories .parents-list ul {
	display: block;
	padding: 0 0 0 62px;
	margin: 0;
	list-style: none;
}
.menu-categories .parents-list ul li {
	display: block;
	margin: 0 0 17px;
}
.menu-categories .parents-list ul li a {
	color: $main-color;
	text-transform: uppercase;
	text-decoration: none;
    font-weight: 600;
	letter-spacing: 1.4px;
}
.menu-categories .parents-list ul li a.active {
	color: #3B4444;
}


.menu-categories .childs-list ul {
	display: block;
	padding: 0 0 0 62px;
	margin: 0;
	list-style: none;
}
.menu-categories .childs-list>ul {
	display: none;
}
.menu-categories .childs-list ul li {
	display: block;
	padding: 0 0 17px;
}
.menu-categories .childs-list ul li a {
	position: relative;
	color: $main-color;
	text-transform: uppercase;
	text-decoration: none;
    font-weight: 600;
	letter-spacing: 1.4px;
}
.menu-categories .childs-list > ul > li > a {
	padding-right: 46px;
}
.menu-categories .childs-list > ul > li > a.has-childs:before {
	content: '';
	display: block;
	position: absolute;
	top: 50%;
	right: 0;
	@include top-arrow (2px, 13px, 13px, #CCD9DA);
	margin-top: -7px;
}
.menu-categories .childs-list > ul > li > a.collapsed:before {
	@include bottom-arrow (2px, 13px, 13px, #CCD9DA);
}
.menu-categories .childs-list ul li ul {
	padding-left: 30px;
}
.menu-categories .childs-list ul li ul.collapse {
	display: none;
}
.menu-categories .childs-list ul li ul.collapse.in {
	display: block;
}
.menu-categories .childs-list ul li ul li a {
	text-transform: none;
	font-weight: 400;
}
.menu-categories .childs-list ul li ul li a:hover {
	color: $a-color;
	text-decoration: underline;
}
.menu-categories .brans-list .item {
	display: none;
}
.menu-categories .brans-list a {
	display: block;
	text-align: center;
	padding: 12px 0;
	text-decoration: none;
	text-transform: uppercase;
	color: $a-color;
}
.menu-categories .brans-list a img {
	max-width: 100%;
}

.form-fancybox-full-screen {
	width: 100%;
	height: 100%;
	background: #EEF2F3;
}
.is-phone .form-fancybox-full-screen {
	overflow-y: scroll;
}

.fancybox-registration .main-title {
	font-size: 54px;
	line-height: 60px;
	font-weight: 300;
	margin-bottom: 70px;
}
.fancybox-registration .wrap-form .btn-submit {
    border-width: 2px;
    font-size: 25px;
    text-transform: uppercase;
    font-weight: 400;
    letter-spacing: 1.9px;
    height: 80px;
}

span.wpcf7-not-valid-tip {
	display: none !important;
}
div.wpcf7-response-output {
	padding: 0 !important;
	margin: 0 !important;
	border: none !important;
}
.wpcf7-not-valid.form-control, .wpcf7-not-valid.form-control .btn-selectpicker {
	border-color: red !important;
}

.fancybox-message {
	max-width: 400px;
	background: #EEF2F3;
	padding: 30px;
}
.scroll-spy {
	background: #EEF2F3;
	position: fixed;
	width: 100%;
	top: 100px;
	left: 0;
	z-index: 10;
}
.scroll-spy ul {
	display: block;
	margin: 0;
	padding: 0;
	list-style: none;
	text-align: center;
}
.scroll-spy ul li {
	display: inline-block;
	margin: 0 15px;
}
.scroll-spy ul li a {
	text-decoration: none;
    text-transform: uppercase;
    color: $main-color;
    display: inline-block;
	padding: 31px 0;
	@include animation-transition (0.5s);
}
.scroll-spy.is-scroll ul li a {
	padding: 5px 0;
	font-size: 14px;
	
}
.scroll-spy ul li a:hover {
	color: $a-color;
}
.scroll-spy ul li.active a {
	color: $a-color;
}

.wrap-input-file {
	position: relative;
    border: 2px dashed #CCD9DA;
    height: 70px;
    font-size: 16px;
    color: #CCD9DA;
    line-height: 26px;
    text-align: center;
}
.wrap-input-file .wrap-input {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.wrap-input-file .wrap-input .wpcf7-form-control-wrap {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.wrap-input-file .wrap-input input {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	@include cros-opacity (0);
}
.wrap-input-file .file-name {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	padding: 7px 0 0;
}


#back-top {
	display: none;
	position: fixed;
    bottom: 100px;
    right: 100px;
}
#back-top a {
	display: inline-block;
	width: 50px;
	height: 50px;
	background: url(../img/arrow-top.png) center center no-repeat #C7E6E8;
	@include border-radius (50%);
	
}


.hidden-input {
	display: none;
}

.blockUI.blockOverlay {
	background: url(../img/loading.gif) center center no-repeat rgba(136, 201, 205, 0.2) !important;
	opacity: 1 !important;
	
}
.woocommerce-Price-currencySymbol {
	font-weight: 400;
}